{
  "name": "frontend-react-vix-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "dc:up": "docker compose up -d --build && docker compose logs -f app",
    "dc:down": "docker compose down --remove-orphans -v",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "doc:build": "docker build -t app-server .",
    "doc:server:up": "docker run --name app-server app-server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.2",
    "@mui/material": "^6.1.2",
    "@mui/x-date-pickers": "^7.23.1",
    "@nivo/geo": "^0.87.0",
    "@react-pdf/renderer": "^4.1.5",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.15.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "lottie-react": "^2.4.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "react": "^18.3.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-i18next": "^15.0.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.5",
    "recharts": "^2.13.3",
    "swiper": "^11.1.14",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@emotion/jest": "^11.13.0",
    "@eslint/js": "^9.9.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^3.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
